# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=debug
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/image2model
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=image2model
POSTGRES_PORT=5432

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=
REDIS_PORT=6379
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Backend Configuration
BACKEND_HOST=localhost
BACKEND_PORT=8000
BACKEND_CORS_ORIGINS=http://localhost:3000

# Frontend Configuration
FRONTEND_PORT=3000
FRONTEND_SSL_PORT=443
DOMAIN_NAME=localhost
VITE_API_URL=http://localhost:8000
NODE_ENV=development

# Worker Configuration
WORKER_CONCURRENCY=2

# AI/ML Model Configuration
MODEL_CACHE_DIR=./models
UPLOAD_DIR=./uploads
RESULTS_DIR=./results

# FAL.AI API Configuration
FAL_KEY_ID=your-fal-key-id
FAL_KEY_SECRET=your-fal-key-secret

# Optional: External API Keys
OPENAI_API_KEY=your-openai-api-key
HUGGING_FACE_API_KEY=your-hugging-face-key

# Development Tools
FLOWER_PORT=5555
FLOWER_BASIC_AUTH=admin:admin
PGADMIN_DEFAULT_EMAIL=admin@image2model.local
PGADMIN_DEFAULT_PASSWORD=admin

# Docker Configuration
COMPOSE_PROJECT_NAME=image2model
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1